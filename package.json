{
  "name": "lyra-cli",
  "version": "0.8.7",
  "description": "Una red de Blockchain usando HyperLedger Fabric para Grupo Financiero Banorte",
  "author": "Aabo Technologies 2017",
  "main": "index.js",
  "scripts": {
    "preinstall": "composer --version || { echo 'Please first run npm install -g composer-cli'; exit 1;} ",
    "start": "scripts/startFabric.sh",
    "stop": "scripts/stopFabric.sh",
    "teardown": "scripts/teardownFabric.sh",
    "deployNetwork": "scripts/initHyper.sh",
    "pingNetwork": "composer network ping -n lyra-cli -p hlfv1 -i admin -s adminpw",
    "install": "scripts/startFabric.sh && npm run deployNetwork",
    "start": "node index.js"
  },
  "keywords": [
    "Blockchain",
    "Solutions",
    "Framework",
    "getting-started",
    "Hyperledger Composer"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "chalk": "^2.2.0",
    "clear": "0.0.1",
    "cli-spinners": "^1.1.0",
    "cli-table": "latest",
    "cluster": "^0.7.7",
    "composer-client": "latest",
    "config": "^1.21.0",
    "digitalproperty-network": "latest",
    "figlet": "^1.2.0",
    "inquirer": "^3.3.0",
    "md5": "^2.2.1",
    "mongodb": "^2.2.33",
    "mongoose": "^4.12.4",
    "ora": "^1.3.0",
    "performance-now": "^2.1.0",
    "prettyoutput": "^1.1.1",
    "shelljs": "^0.7.5",
    "winston": "^2.2.0",
    "yargs": "^6.5.0"
  }
}
